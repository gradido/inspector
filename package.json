{
  "name": "gradido-blockchain-inspector",
  "version": "1.0.0",
  "main": "index.js",
  "author": "einhornimmond <info@einhornimmond.de>",
  "license": "MIT",
  "scripts": {
    "predev": "bun locale:de",
    "dev": "cross-env TZ=UTC bunx --bun vite",
    "prebuild": "bun locale:de",
    "build": "NODE_ENV=production bunx --bun vite build",
    "preserve": "bun locale:de",
    "serve": "cross-env TZ=UTC bunx --bun vite preview",
    "typecheck": "bun typecheck",
    "lint": "biome check --error-on-warnings .",
    "extract": "bunx --bun gettext-extract",
    "locale:de": "node node_modules/gettext.js/bin/po2json src/locales/de/messages.po src/locales/de/messages.json"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "cross-env": "^7.0.3",
    "gettext.js": "^2.0.3",
    "mithril": "^2.2.2",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.0",
    "@mojojoejo/vite-plugin-purgecss": "^1.1.0",
    "@popperjs/core": "^2.11.8",
    "@types/bootstrap": "^5.2.10",
    "@types/gettext.js": "^1.0.3",
    "@types/minimatch": "^6.0.0",
    "@types/mithril": "^2.2.7",
    "dotenv": "^17.2.2",
    "gettext-extract": "^2.0.1",
    "less": "^4.4.2",
    "minimatch": "^10.0.3",
    "sass-embedded": "^1.93.2",
    "typescript": "^5.6.2",
    "unplugin-icons": "^22.3.0",
    "unplugin-vue-components": "^29.1.0",
    "vite": "^7.1.7",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-html": "^3.2.2"
  }
}
